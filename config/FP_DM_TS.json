          "kwargs": {
            "gamma_sigma":  0.02,
            "init_gamma": 1.0,
            "inference": "\"policy\"",
            "context_dim": 5,
            "exploration_noise": 0.02
          }
    }
  }],
  "output_dir": "results/FP_DM(context)_TS_FO/"
}
